#include<stdlib.h>
#include<string.h>
#include <stdio.h>

struct dic{
    char em[10];
    char name[10];
};
struct node{
    struct dic m;
    struct node *next;
};


struct node *a[100];

struct node *last(struct node *h)
{
    
    while(h->next!=NULL)
    {
        h=h->next;
    }
    return h;
}
int len(struct node *h)
{
    int c=0;
    while(h!=NULL)
    {
        c++;
        h=h->next;
    }
    return c;
}








struct node  *createnode()
{
    struct dic b;
    struct node *h;
    h=(struct node*)malloc(sizeof(struct node));
    printf("enter key\n");
    scanf("%s",b.em);
    h->m=b;
    h->next=NULL;
    return h;
}


 

    
    

struct node *update()
{
    struct node *t,*t2,*h,*l,*z;
    t=createnode();
    char c;
    c=t->m.em[0];
    int d,x;
    d=(int)c-(int)'0'-49;
    x=d%100;
    h=a[x];
    if(a[x]==NULL)
    {
        a[x]=t;
        h=a[x];
    }
    else{
        l=last(a[x]);
        
        if(len(a[x])==1){
            
                if(strcmp(t->m.em,a[x]->m.em)>0)
                {
                    a[x]->next=t;
                
                }
                else{
                    t->next=a[x];
                    a[x]=t;
                    
                }
            }
            
else if(strcmp(t->m.em,l->m.em)>0)
            {
                
                l->next=t;
            }
else{
while(a[x]!=NULL)
        {
            
    
            z=a[x]->next;
            if(strcmp(t->m.em,a[x]->m.em)>0 && strcmp(t->m.em,z->m.em)<0)
            {
                t->next=z;
                a[x]->next=t;
                break;
            }
            a[x]=a[x]->next;
        
            
        }
}
    }
    t2=h;
    return h;

    
}













void print(struct node *h)
{
    while(h!=NULL)
     {
         printf("%s->",h->m.em);
         h=h->next;

     }
     }
int main()
{
    struct node *t;
    //printf("enter key");
    t=update();
    t=update();
    t=update();
    
     print(t);   
}
